@using Starter.Feature.Navigation.Models
@model BreadcrumbItemList

<ol class="breadcrumb">
    @foreach (var item in Model.Breadcrumbs)
    {
        <li>
            @if (@item.IsActive)
            {@item.Title }
            else
            {
                <a href="@item.Url">
                    @item.Title
                </a>
            }
        </li>
    }
</ol>
